<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <class name="LotterySystem.Po.Door,LotterySystem" table="door" lazy="true" >

    <composite-id>
      <key-property name="RoomName" column="ROOM_NAME" type="string" ></key-property>
      <key-property name="GameName" column="GAME_NAME" type="string" ></key-property>
      <key-property name="UserName" column="USER_NAME" type="string" ></key-property>
    </composite-id>
 
    <property name="EntranceType" type="string">
      <column name="ENTRANCE_TYPE" length="50"/>
    </property>
 
  </class>
</hibernate-mapping>