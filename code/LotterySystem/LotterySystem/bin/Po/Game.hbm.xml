<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <class name="LotterySystem.Po.Game,LotterySystem" table="game" lazy="true" >


    <id name="GameName" column ="GAME_NAME" >
      
    </id>


    <property name="GameStatus" type="string">
      <column name="GAME_STATUS" length="50"/>
    </property>
    
    <property name="Os" type="string">
      <column name="OS" length="50"/>
    </property>

    <property name="Browser" type="string">
      <column name="BROWSER" length="50"/>
    </property>
    
    <property name="TaxRate" type="float">
      <column name="TAX_RATE" length="50"/>
    </property>

    <property name="AllRoomLimit" type="int">
      <column name="ALL_ROOM_LIMIT" length="50"/>
    </property>

    <property name="OnePersonRoomLimit" type="int">
      <column name="ONE_PERSON_ROOM_LIMIT" length="50"/>
    </property>

    <property name="OneTablePersonLimit" type="int">
      <column name="ONE_TABLE_PERSON_LIMIT" length="50"/>
    </property>

    <property name="OneRoomTableLimit" type="int">
      <column name="ONE_ROOM_TABLE_LIMIT" length="50"/>
    </property>
    
    <property name="BankerSelectNumTimeLimit" type="int">
      <column name="BANKER_SELECT_NUM_TIME_LIMIT" length="50"/>
    </property>
    
    <property name="PlayerSelectNumTimeLimit" type="int">
      <column name="PLAYER_SELECT_NUM_TIME_LIMIT" length="50"/>
    </property>

  </class>
</hibernate-mapping>