
<!DOCTYPE html>
<html lang="cn">
	<head>
		<title>@ViewBag.Title</title>
		<meta http-equiv="content-type" content="text/html; charset=UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />

        <link href="~/Content/icon/icon.ico" rel="shortcut icon" type="image/x-icon" />
       	<link rel="stylesheet" href="css/bootstrap.css" />

        @Styles.Render("~/Content/bootstrap/css/bootstrap.css")
        @Styles.Render("~/Content/bootstrap/css/bootstrap.min.css")
         @Styles.Render("~/Content/bootstrap/css/bootstrap-responsive.min.css")
        @Scripts.Render("~/bundles/modernizr")    
        @Scripts.Render("~/bundles/bootstrapjs")  
        @Scripts.Render("~/Scripts/jquery-1.8.3.min.js")
        @Scripts.Render("~/Scripts/jquery.unobtrusive-ajax.min.js")

    </head>
    <body>
       
        <div class="container">


   
                



            <div id="body">
                @RenderSection("featured", required: false)
                <section class="content-wrapper main-content clear-fix">
                    @RenderBody()
                </section>
            </div> 
         </div>   
             

    </body>
</html>
