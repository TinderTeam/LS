<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <class name="LotterySystem.Po.ScoreLog,LotterySystem" table="score_log"  >

    
 
    <id name="LogID" column ="LOG_ID" >
      <generator class ="increment"/>
    </id>

    <property name="UserName" type="string">
      <column name="USER_NAME"   />
    </property>
    
    <property name="RecordTime" type="DateTime">
      <column name="RECORD_TIME" />
    </property>
    
    <property name="Value" type="int">
      <column name="VALUE"  />
    </property>
    
    <property name="Balance" type="int">
      <column name="BALANCE"  />
    </property>  
    
    <property name="Mode" type="string">
      <column name="MODE" length="50"/>
    </property>

    <property name="GamblingPartyID" type="long">
      <column name="GAMBLING_PARTY_ID" />
    </property>

    <property name="RoundID" type="long">
      <column name="ROUND_ID"  />
    </property>


    <property name="OtherName" type="string">
      <column name="OTHER_NAME" />
    </property>




  </class>
</hibernate-mapping>