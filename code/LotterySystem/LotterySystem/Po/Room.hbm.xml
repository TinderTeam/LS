<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <class name="LotterySystem.Po.Room,LotterySystem" table="room" lazy="true" >

    <composite-id>
      <key-property name="RoomName" column="ROOM_NAME" type="string" ></key-property>
      <key-property name="GameName" column="GAME_NAME" type="string" ></key-property>
    </composite-id>
 
 
     
    <property name="RoomPassword" type="string">
      <column name="ROOM_PASSWORD" length="50"/>
    </property>

    <property name="RoomHost" type="string">
      <column name="ROOM_HOST" length="50"/>
    </property>

    <property name="Status" type="string">
      <column name="STATUS" length="50"/>
    </property>


    <property name="AccessAcountLimit" type="int">
      <column name="ACCESS_ACOUNT_LIMIT" length="50"/>
    </property>
 

    <property name="BasicPoint" type="int">
      <column name="BASIC_POINT" length="50"/>
    </property>

    <property name="Amplification" type="int">
      <column name="AMPLIFICATION" length="50"/>
    </property>
    
  </class>
</hibernate-mapping>